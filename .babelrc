{
  "presets": [
    [
      "env",
      {
        "targets": { "node": "6.10" },
        "useBuiltIns": true,
        "modules": false,
        "loose": true
      }
    ],
    "stage-0"
  ],
  "env": {
    "production": {
      "presets": [["minify"]]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": { "node": "6.10" },
            "useBuiltIns": true,
            "modules": "commonjs",
            "loose": true
          }
        ]
      ]
    }
  },
  "plugins": ["source-map-support", "transform-flow-strip-types"]
}
